{
  "name": "enqueuer",
  "version": "1.0.10",
  "description": "Asynchronous messages testing tool",
  "main": "index.js",
  "scripts": {
    "start": "node js/index.js",
    "test": "npm run unitTest && npm run integrationTest",
    "unitTest": "jest --forceExit",
    "integrationTest": "npm start -- -c integrationTest/enqueuer.yml",
    "lint": "node_modules/.bin/tslint --project tsconfig.json --force",
    "clean": "rm -rf js/ && find ./src -name '*.js' -type f -delete && rm -f temp/*",
    "all": "npm run clean && npm run build && npm link && npm run test",
    "build": "./scripts/generate-injectables-list.sh src/injectable-files-list.ts src && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lopidio/enqueuer"
  },
  "author": "Virgs",
  "license": "MIT",
  "bin": {
    "enqueuer": "js/index.js"
  },
  "dependencies": {
    "@types/chokidar": "^1.7.5",
    "@types/express": "^4.16.0",
    "@types/object-hash": "^1.2.0",
    "@types/prettyjson": "0.0.28",
    "@types/request": "^2.47.1",
    "ajv": "^6.2.1",
    "amqp": "^0.2.6",
    "aws-sdk": "^2.255.1",
    "chokidar": "^2.0.2",
    "commander": "^2.14.1",
    "conditional-injector": "^1.0.4",
    "express": "^4.16.3",
    "json-placeholder-replacer": "^1.0.12",
    "log4js": "^2.5.3",
    "mqtt": "^2.17.0",
    "object-hash": "^1.3.0",
    "prettyjson": "^1.2.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/amqp": "^0.2.4",
    "@types/yamljs": "^0.2.30",
    "@types/jest": "^22.2.3",
    "@types/node": "^9.4.6",
    "jest": "^22.4.2",
    "ts-jest": "^22.4.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.10.0",
    "typescript": "^2.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": ".*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}

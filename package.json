{
  "name": "enqueuer",
  "version": "1.0.4",
  "description": "Desktop client to send and receive messages from queue systems",
  "main": "index.js",
  "scripts": {
    "start": "node js/index.js",
    "test": "jest --forceExit && npm start -- -c integrationTest/enqueuer.yml",
    "clean": "rm -rf js/ && find ./src -name '*.js' -type f -delete && rm -f temp/*",
    "all": "npm run clean && npm run build && npm link && npm tst",
    "build": "./scripts/generate-injectables-list.sh src/injectable-files-list.ts src && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lopidio/enqueuer"
  },
  "author": "Virgs",
  "license": "MIT",
  "bin": {
    "enqueuer": "js/index.js"
  },
  "dependencies": {
    "ajv": "^6.2.1",
    "amqp": "^0.2.6",
    "chokidar": "^2.0.2",
    "commander": "^2.14.1",
    "conditional-injector": "^1.0.3",
    "express": "^4.16.3",
    "json-placeholder-replacer": "^1.0.11",
    "log4js": "^2.5.3",
    "mqtt": "^2.17.0",
    "npm": "^5.8.0",
    "object-hash": "^1.3.0",
    "prettyjson": "^1.2.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/node": "^9.4.6",
    "jest": "^22.4.2",
    "ts-jest": "^22.4.1",
    "ts-node": "^4.1.0",
    "tsd": "^0.6.5",
    "typescript": "^2.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": ".*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}

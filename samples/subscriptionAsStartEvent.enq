{
  "timeout": "10000",
  "subscriptions": [
    {
      "protocol": "mqtt",
      "brokerAddress": "brokerAddress",
      "topic": "subscription",
      "onMessageReceivedFunctionBody": "report['reportMessage'] = 'reportMessage'; test['true'] = true; test['false'] = false",
      "timeout": "8000"
    }
  ],
  "startEvent": {
    "subscription":
    {
      "protocol": "amqp",
      "brokerUrl": "amqp://localhost:5672",
      "exchangeName": "ENQUEUER.E",
      "routingKey": "#",
      "queueName": "ENQUEUER.SUBS"
    }
  },
  "reports" : [
    {
      "protocol": "standardOutput"
    },
    {
      "protocol": "file",
      "filename": "outputFileNameReportBack"
    },
    {
      "protocol": "amqp",
      "url": "amqp://localhost:5672",
      "queue": "ENQUEUER.REPORT"
    },
    {
      "protocol": "http",
      "endpoint": "http://localhost:1880/reports"
    },
    {
      "protocol": "mqtt",
      "brokerAddress": "brokerAddress",
      "topic": "service-to-be-tested/report"
    }
  ],
  "variables":
  {
    "brokerAddress": "mqtt://localhost"
  }
}
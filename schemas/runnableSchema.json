{
  "$id": "https://github.com/lopidio/enqueuer/runnable",
  "type": "object",
  "properties": {
    "initialDelay": {
      "type": "number",
      "title": "The requisition initialDelay",
      "default": "",
      "examples": [
        5000,
        60000
      ]
    },
    "iterations": {
      "type": "number",
      "title": "The requisition iterations amount",
      "default": "",
      "examples": [
        1,
        3
      ]
    },
    "name": {
      "type": "string",
      "title": "The requisition name",
      "default": "",
      "examples": [
        "anyNameOrTitle"
      ]
    },
    "runnableVersion": {
      "type": "string",
      "title": "The runnable version",
      "default": "",
      "examples": [
        "01.00.00"
      ]
    },
    "runnables": {
      "type": "array",
      "additionalItems": false,
      "items": {
        "oneOf": [
          {"$ref": "https://github.com/lopidio/enqueuer/runnable"},
          {"$ref": "https://github.com/lopidio/enqueuer/requisition"}
        ]
      }
    },
    "additionalProperties": false
  },
  "required": [
    "collectionVersion",
    "runnables"
  ]
}
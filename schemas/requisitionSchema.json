{
  "$id": "https://github.com/lopidio/enqueuer/requisition",
  "type": "object",
  "properties": {
    "timeout": {
      "type": "number",
      "title": "The requisition timeout",
      "default": "",
      "examples": [
        5000, 60000
      ]
    },
    "name": {
      "type": "string",
      "title": "The requisition name",
      "default": "",
      "examples": [
        "anyNameOrTitle"
      ]
    },
    "subscriptions": {
      "type": "array",
      "items": { "$ref": "https://github.com/lopidio/enqueuer/subscription" }
    },
    "startEvent": {
      "type": "object",
      "properties": {
        "subscription": {
          "$ref": "https://github.com/lopidio/enqueuer/subscription"
        },
        "publisher": {
          "$ref": "https://github.com/lopidio/enqueuer/publisher"
        }
      },
      "additionalProperties": false,
      "oneOf": [
        { "required": [ "subscription" ] },
        { "required": [ "publisher" ] }
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "subscriptions",
    "startEvent"
  ]
}
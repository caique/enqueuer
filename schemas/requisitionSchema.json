{
  "$id": "https://github.com/lopidio/enqueuer/requisition",
  "type": "object",
  "properties": {
    "timeout": {
      "type": "number",
      "title": "The requisition timeout",
      "default": "",
      "examples": [
        5000, 60000
      ]
    },
    "requisitionVersion": {
      "type": "string",
      "title": "The requisition version",
      "default": "",
      "examples": [
        "01.00.00"
      ]
    },
    "subscriptions": {
      "type": "array",
      "items": { "$ref": "https://github.com/lopidio/enqueuer/subscription" }
    },
    "startEvent": {
      "type": "object",
      "properties": {
        "subscription": {
          "$ref": "https://github.com/lopidio/enqueuer/subscription"
        },
        "publisher": {
          "$ref": "https://github.com/lopidio/enqueuer/publisher"
        }
      },
      "additionalProperties": false,
      "oneOf": [
        { "required": [ "subscription" ] },
        { "required": [ "publisher" ] }
      ]
    },
    "reports": {
      "type": "array",
      "items": { "$ref": "https://github.com/lopidio/enqueuer/publisher" }
    },
    "variables": {
      "type": "object",
      "properties": {
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "requisitionVersion",
    "subscriptions",
    "startEvent",
    "reports"
  ]
}
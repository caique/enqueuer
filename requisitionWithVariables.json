{
    "protocol": "mqtt",
    "brokerAddress": "mqtt://localhost",
    "subscriptions": [
        {
            "topic": "service-to-be-tested/output/1",
            "testFunctionBody": "test[{{keyStr}}] = false; console.log('valVar: ' + {{keyStr}})"
        },
        {
            "timeout": "{{keyNum}}",
            "topic": "service-to-be-tested/output/2"
        }
    ],
    "publish": 
    {
      "topic": "service-to-be-tested/input",
      "payload": "{{keyVar}}"
    },
    "variables":
    {
        "keyStr": "valStr",
        "keyNum": 100
    }
}
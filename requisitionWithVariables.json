{
    "protocol": "mqtt",
    "brokerAddress": "mqtt://localhost",
    "subscriptions": [
        {
            "topic": "service-to-be-tested/output/1",
            "onMessageReceived": "test[keyStr}}] = false; console.log('valVar: ' + 'keyStr')"
        },
        {
            "timeout": "keyNum",
            "topic": "service-to-be-tested/output/2"
        }
    ],
    "publish": 
    {
        "topic": "service-to-be-tested/input",
        "payload": "keyStr"
    },
    "variables":
    {
        "keyStr": "valStr",
        "keyNum": 100
    }
}
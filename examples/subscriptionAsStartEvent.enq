{
  "requisitionVersion": "01.00.00",
  "timeout": 10000,
  "subscriptions": [
    {
      "type": "mqtt",
      "brokerAddress": "brokerAddress",
      "topic": "subscription",
      "onMessageReceived": "report['reportMessage'] = 'reportMessage'; test['true'] = true; test['false'] = false",
      "timeout": 8000
    }
  ],
  "startEvent": {
    "subscription":
    {
      "type": "amqp",
      "brokerUrl": "amqp://localhost:5672",
      "exchangeName": "ENQUEUER.E",
      "routingKey": "#",
      "queueName": "ENQUEUER.SUBS"
    }
  },
  "reports" : [
    {
      "type": "standard-output"
    },
    {
      "type": "file",
      "filenamePrefix": "output/outputFileNameReportBack"
    },
    {
      "type": "amqp",
      "url": "amqp://localhost:5672",
      "queue": "ENQUEUER.REPORT"
    },
    {
      "type": "http-client",
      "url": "http://localhost:1880/reports"
    },
    {
      "type": "mqtt",
      "brokerAddress": "brokerAddress",
      "topic": "service-to-be-tested/report"
    }
  ],
  "variables":
  {
    "brokerAddress": "mqtt://localhost"
  }
}
{
  "requisitionVersion": "01.00.00",
  "timeout": 3000,
  "subscriptions": [
    {
      "type": "mqtt",
      "brokerAddress": "brokerAddress",
      "topic": "enqueuer/output",
      "onMessageReceived": "test['works'] = message == 100;",
      "timeout": 1000
    }
  ],
  "startEvent": {
    "publisher": {
      "type": "mqtt",
      "brokerAddress": "brokerAddress",
      "payload": 100,
      "topic": "enqueuer/input"
    }
  },
  "reports": [
    {
      "type": "mqtt",
      "brokerAddress": "mqtt://localhost",
      "topic": "enqueuer/report"
    },
    {
      "type": "file",
      "filenamePrefix": "report"
    }
  ],
  "variables":
  {
    "brokerAddress": "mqtt://localhost"
  }
}

{
    "protocol": "mqtt",
    "brokerAddress": "brokerAddress",
    "subscriptions": [
        {
          "mqtt": {
            "brokerAddress": "brokerAddress",
            "topic": "#",
            "onMessageReceivedFunctionBody": "report['startEvent'] = startEvent.topic",
            "timeout": "5000"
          }
        }],
    "startEvent": {
        "publish": 
        {
            "rest": {
                "endpoint": "http://localhost:1880/input",
                "method": "POST",
                "payload": "Whad do you mean?",
                "header": {
                },
                "prePublishing": "console.log('brokerAddress: ' + 'brokerAddress');"
            }
        },
        "timeout": "5000"
    },
    "reports" : {
        "standardOutput": {},
        "file": {
            "name": "outputFileNameReportBack"
        },
        "http": {
            "endpoint": "http://localhost:1880/reports"
        },
        "mqtt": 
        {
            "brokerAddress": "brokerAddress",
            "topic": "service-to-be-tested/report"
        }
    },
    "variables":
    {
        "brokerAddress": "mqtt://localhost"
    }
}
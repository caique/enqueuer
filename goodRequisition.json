{
    "protocol": "mqtt",
    "brokerAddress": "brokerAddress",
    "subscriptions": [
        {
            "topic": "service-to-be-tested/output/1",
            "onMessageReceived": "report['startEvent'] = startEvent.topic"
        },
        {
            "timeout": "5000",
            "topic": "service-to-be-tested/output/2"
        }],
    "startEvent": {
        "publish": 
        {
            "payload": "keyStr",
            "rest": {
                "endpoint": "http://blablaservice-to-be-tested:8080/input",
                "method": "POST",
                "header": {

                },
                "prePublishing": "console.log('valVar: ' + 'keyStr');"
            }
        },
        "timeout": "2000"
    },
    "reports" : {
        "standardOutput": {},
        "file": {
            "name": "outputFileNameReportBack"
        },
        "http": {
            "endpoint": "http://localhost:1880/reports"
        },
        "mqtt": 
        {
            "brokerAddress": "brokerAddress",
            "topic": "service-to-be-tested/report"
        }
    },
    "variables":
    {
        "brokerAddress": "mqtt://localhost"
    }
}
{
    "protocol": "mqtt",
    "brokerAddress": "mqtt://localhost",
    "subscriptions": [
        {
            "topic": "service-to-be-tested/output/1",
            "onMessageReceived": "report['startEvent'] = startEvent.topic"
        },
        {
            "timeout": "5000",
            "topic": "service-to-be-tested/output/2"
        }],
    "startEvent": {
        "publish": 
        {
            "mqtt": {
                "topic": "service-to-be-tested/input",
                "payload": "payload",
                "brokerAddress": "mqtt://localhost"                
            }
        }
    },
    "report" : {
        "standardOutput": {},
        "file": {
            "name": "outputFileNameReportBack"
        },
        "http": {
            "endpoint": "http://localhost:1880/reports"
        },
        "mqtt": 
        {
            "brokerAddress": "mqtt://localhost",
            "topic": "service-to-be-tested/report"
        }
    }
}
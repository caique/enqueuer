{
  "requisitionVersion": "01.00.00",
  "timeout": 3000,
  "subscriptions": [
    {
      "type": "amqp",
      "options": {
        "host": "brokerUrl",
        "port": 5672
      },
      "queueName": "enqueuerQueue",
      "onMessageReceived": "test['works'] = JSON.parse(message).works ;",
      "timeout": 1000
    }
  ],
  "startEvent": {
    "publisher": {
      "type": "amqp",
      "options": {
        "host": "brokerUrl",
        "port": 5672
      },
      "queueName": "enqueuerQueue",
      "payload": {"works":true}
    }
  },
  "reports": [
    {
      "type": "standard-output"
    }
  ],
  "variables":
  {
    "brokerUrl": "localhost"
  }
}

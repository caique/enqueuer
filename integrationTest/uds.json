{
  "runnableVersion": "01.00.00",
  "name": "runnableUDSName",
  "initialDelay": 0,
  "runnables": [
    {
      "timeout": 3000,
      "name": "uds",
      "subscriptions": [
        {
          "type": "uds",
          "path": "/tmp/unix.sock",
          "name": "udsSubscription",
          "onMessageReceived": "message = JSON.parse(message); test['Message received is \\'' + '{{udsPayload}}?\\''] = message === '{{udsPayload}}';",
          "timeout": 500
        }
      ],
      "startEvent": {
        "publisher": {
          "type": "uds",
          "name": "udsPublisher",
          "path": "/tmp/unix.sock",
          "payload": "{{udsPayload}}",
          "prePublishing": "const payload = 'enqueuer'; persistSessionVariable('udsPayload', payload);"
        }
      }
    }
  ]
}
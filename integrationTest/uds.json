{
  "runnableVersion": "01.00.00",
  "iterations": 1,
  "name": "runnableUDSName",
  "initialDelay": 0,
  "runnables": [
    {
      "timeout": 3000,
      "name": "uds",
      "subscriptions": [
        {
          "type": "uds",
          "path": "/tmp/unix.sock",
          "name": "udsSubscription",
          "onMessageReceived": "test['first letter'] = message.substring(1,2) === 'e'; console.log('Message: ' + message)",
          "timeout": 500
        }
      ],
      "startEvent": {
        "publisher": {
          "type": "uds",
          "name": "udsPublisher",
          "path": "/tmp/unix.sock",
          "payload": "enqueuer"
        }
      }
    }
  ]
}
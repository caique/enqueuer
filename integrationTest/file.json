{
  "requisitionVersion": "01.00.00",
  "timeout": 3000,
  "subscriptions": [
    {
      "type": "file-name-watcher",
      "fileNamePattern": "integrationTest/fileTest*file",
      "onMessageReceived": "test['some time has passed'] = new Date().getTime() + '' >= message;",
      "timeout": 1000
    }
  ],
  "startEvent": {
    "publisher": {
      "type": "file",
      "filenamePrefix": "integrationTest/fileTest",
      "filenameExtension": "file",
      "prePublishing": "publisher.payload=new Date().getTime();"
    }
  }
}
